{
	"manifest_version": 2,
	"name": "autognity_extension",
	"version": "1.0",
	"permissions": ["tabs","storage"],
	"content_scripts": [{
		"run_at": "document_end",
		"matches": ["http://localhost/com.admin.rest/*","https://localhost/com.admin.rest/*"],
		"css": ["css/styles.css"],
		"js": ["js/jquery-3.4.1.min.js","js/forge.all.js","js/content.js"]
	}],
	"background": {
		"scripts": ["js/jquery-3.4.1.min.js","js/forge.all.js","js/params.js","js/background.js"]
	},
	"browser_action": {
		"default_title": "Clicca QUI!",
		"default_icon": "img/icon.png",
		"default_popup":"popup.html"
	}
}
